<!DOCTYPE html>
<html lang="de">
  <%= render 'layouts/head' %>
  <body>
    <%= render 'navigation/navbar_company_view' %>
    <div class="wrapper">

      <!-- Sidebar -->
      <nav id="sidebar" class="bg-light pt-3">
        <%= render 'navigation/side_nav_company' %>
      </nav>

      <!-- Page Content -->
      <div id="content">
        <div class="container-fluid">
          <%= yield %>
        </div>
      </div>

    </div>
    <div class="modal" role="dialog" id="dynamicModal">
      <div class="modal-dialog" id="modal-content" role="document">

      </div>
    </div>
    <%= custom_bootstrap_flash %>
  </body>
</html>
